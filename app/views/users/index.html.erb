
<%= render 'books/userinfo' , user: @user %>

<h1>Users</h1>
<table>
<tr>
  <th>image</th>
  <th>name</th>
  <th></th>
</tr>

<% @users.each do |user| %>
<tr>
<th><%= image_tag user.get_profile_image(100,100) %></th>

<th><%= user.name %></th>

<th><%= link_to "Show", user_path(user.id) %></th>

</tr>
<% end %>
</table>
